    "heading_f00004f1d1353fcf9165db468da83bd9": {
      "type": "heading",
      "id": "heading_f00004f1d1353fcf9165db468da83bd9",
      "source_id": "",
      "content": "Known issues",
      "level": 1
    },
    "21592bc5693abc92f529b3cdb06652c7": {
      "type": "text",
      "id": "21592bc5693abc92f529b3cdb06652c7",
      "source_id": "",
      "content": "In rare conditions there is the possibilty that the displayed content gets out of sync with your input. If Substance Composer exhibites this behavior, please first save your article using ⌘+S. Then, press ⌘+R to reload the interface. If you do encounter this issue and would like to detail the steps taken to reproduce it, please send a bug report to support@substance.io. We'll try to address them as soon as possible."
    },
