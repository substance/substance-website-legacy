    "heading_af22716dcd6f88afdc616d184aa0b019": {
      "type": "heading",
      "id": "heading_af22716dcd6f88afdc616d184aa0b019",
      "source_id": "",
      "content": "How can I publish a collection of documents?",
      "level": 2
    },
    "59ae0182bcdbfd6c066f59823082b872": {
      "type": "text",
      "id": "59ae0182bcdbfd6c066f59823082b872",
      "source_id": "",
      "content": ".sdf files are just zip archives. You can rename them to .zip and extract its contents. Inside you'll find a content.json file plus assets like images."
    },
    "code_409db3141e234251fcdc106ab7c37698": {
