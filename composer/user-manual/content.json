    },
    "figure_b1004b3a491fe26a8d0729510db2b4c3": {
      "type": "figure",
      "id": "figure_b1004b3a491fe26a8d0729510db2b4c3",
      "image": "figure_b1004b3a491fe26a8d0729510db2b4c3.gif",
