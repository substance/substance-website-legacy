    "heading_8c5d5bd3b94ce448710ac0f36e8effff": {
      "type": "heading",
      "id": "heading_8c5d5bd3b94ce448710ac0f36e8effff",
      "source_id": "",
      "content": "Review",
      "level": 1
    },
    "db2ca106b812ec016f1cb5ace75fb683": {
      "type": "text",
      "id": "db2ca106b812ec016f1cb5ace75fb683",
      "source_id": "",
      "content": "Substance Composer supports an offline review workflow. You can send your-document.sdf to a friend and let them review your content. They'll be able to add remarks and corrections, but also edit the text itself."
    },
    "ec72d91c3abd3fdb29e437411b4e48c0": {
      "type": "text",
      "id": "ec72d91c3abd3fdb29e437411b4e48c0",
      "source_id": "",
      "content": "Open Substance Composer. You will be greeted by a blank document."
    },
    "heading_69c95f3cfcc27f52ef2af04e7398b7d0": {
      "type": "heading",
      "id": "heading_69c95f3cfcc27f52ef2af04e7398b7d0",
      "source_id": "",
      "content": "Add structure",
      "level": 2
    },
    "c6e2a7b439a00e068f73357546b9bebd": {
      "type": "text",
      "id": "c6e2a7b439a00e068f73357546b9bebd",
      "source_id": "",
      "content": "Use the text transform tool on the upper left to turn a paragraph into a heading in order to structure your document. To change the level of a heading you can also use TAB and Shift+TAB keyboard shortcuts."
    },
    "67a1ecf64eccbdc6543d5ef31782dd3d": {
      "type": "text",
      "id": "67a1ecf64eccbdc6543d5ef31782dd3d",
      "source_id": "",
      "content": "Substance Composer is a modern workspace for creating professional web-first publications. The app works similarly to traditional word processors. There is one important difference though: Substance Composer urges you to focus only on your writing, not on its layout."
    },
    "f5aec945d9a1255a623fd59c61157a74": {
      "type": "text",
      "id": "f5aec945d9a1255a623fd59c61157a74",
      "source_id": "",
      "content": "Move the cursor using mouse or arrow keys, and enter text as you would in any text editing environment."
    },
    "heading_6a91fbce782d8144b7bf40a276a13e29": {
      "type": "heading",
      "id": "heading_6a91fbce782d8144b7bf40a276a13e29",
      "source_id": "",
      "content": "Add remarks",
      "level": 2
    },
    "heading_f83e0894cfeb0c32251dc392eeb44b67": {
      "type": "heading",
      "id": "heading_f83e0894cfeb0c32251dc392eeb44b67",
      "source_id": "",
      "content": "Emphasize your text",
      "level": 2
    },
    "heading_d2824dd68b7a96838598abeee98bf79e": {
      "type": "heading",
      "id": "heading_d2824dd68b7a96838598abeee98bf79e",
      "source_id": "",
      "content": "Frequently Asked Questions",
      "level": 1
    },
    "heading_08c3a868e881410d2e61bf95e4e36df8": {
      "type": "heading",
      "id": "heading_08c3a868e881410d2e61bf95e4e36df8",
      "source_id": "",
      "content": "Installation",
      "level": 1
    },
    "5ecacc5903cdf8fb939218ea384462de": {
      "type": "text",
      "id": "5ecacc5903cdf8fb939218ea384462de",
      "source_id": "",
      "content": "Execute setup.exe and follow the installation instructions."
    },
    "heading_e1a859e5ec21e01ae5d6f067fc72aaac": {
      "type": "heading",
      "id": "heading_e1a859e5ec21e01ae5d6f067fc72aaac",
      "source_id": "",
      "content": "OSX",
      "level": 2
    },
    "heading_4b506dff71ecf260b0a7c5c8959a6d1c": {
      "type": "heading",
      "id": "heading_4b506dff71ecf260b0a7c5c8959a6d1c",
      "source_id": "",
      "content": "Windows",
      "level": 2
    },
    "117bddecdbcaf967f63a53f663f8f26d": {
      "type": "text",
      "id": "117bddecdbcaf967f63a53f663f8f26d",
      "source_id": "",
      "content": "Extract the contents of the zip file and drag Substance Composer.app to your Applications folder."
    },
    "code_3f02dab8b14af6aeeaf8b1b9016898f2": {
      "type": "code",
      "id": "code_3f02dab8b14af6aeeaf8b1b9016898f2",
