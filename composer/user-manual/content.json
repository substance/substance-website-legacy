    "heading_985892b0d5c46962ad1e364f4437d826": {
      "type": "heading",
      "id": "heading_985892b0d5c46962ad1e364f4437d826",
      "source_id": "",
      "content": "Linux",
      "level": 2
    },
    "e6182870afc07446fa8e90ef9dba1717": {
      "type": "text",
      "id": "e6182870afc07446fa8e90ef9dba1717",
      "source_id": "",
      "content": "Linux version is available. Installation instructions are coming soon."
    },
    "9f25ce0624f78bc45b705fe4404d3bd5": {
      "type": "text",
      "id": "9f25ce0624f78bc45b705fe4404d3bd5",
      "source_id": "",
      "content": "We provide a limited set of annotations to emphasize your content with different semantics. Use the annotation toolbar on the uper right of the content panel to do so."
    },
    "1075422960dada279f49af9186c42206": {
      "type": "text",
      "id": "1075422960dada279f49af9186c42206",
      "source_id": "",
      "content": "Sunstance Composer doesn't yet support online collaboration. However, you can share documents in a Dropbox or Google Drive folder, so that your co-authors and editors can access and modify them."
    },
    "27a7b3e7cc005a97610683315626ec4c": {
      "type": "text",
      "id": "27a7b3e7cc005a97610683315626ec4c",
      "source_id": "",
      "content": "First you need to download and install the Substance Composer on your computer."
    },
